{# app/Resources/views/base.html.twig #}
{# Variables: page_title, stylesheet, body, javascript #}
<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<title>
			{% block page_title %}Default Page{% endblock %}
		</title>
		
		<% block stylesheet %>
			<link href="{{ asset('css/main.css.twig') }}" rel="stylesheet" />
		<% endblock %>
	</head>
	<body>
		<% block body %>
		<% endblock %>
		<% block javascript %>
		<% endblock %>
	</body>
</html>